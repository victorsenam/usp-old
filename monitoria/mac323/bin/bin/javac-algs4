#!/bin/bash

# *************************************************
# javac-algs4
# Hayk Martirosyan
# -------------------
# Wrapper for javac that includes algs4.jar.
# *************************************************

# Sets the path to the classpath libraries
jars=(.:${MAC323_INSTALL}/algs4.jar:${MAC_323INSTALL}/stdlib.jar)

javac -cp "$jars" -g -encoding UTF-8 "$@"
